## Virtualization
CONFIG_VIRTUALIZATION=y
CONFIG_KVM=y
## BRANCH_PREDICTOR needed to avoid KVM build crash
CONFIG_HARDEN_BRANCH_PREDICTOR=y

## Watchdog
CONFIG_WATCHDOG=y
CONFIG_WATCHDOG_CORE=y
CONFIG_WATCHDOG_NOWAYOUT=y

## Enable sync framework options
CONFIG_SYNC=y
CONFIG_SW_SYNC=y
CONFIG_SW_SYNC_USER=y
CONFIG_ONESHOT_SYNC=y

## FW loader from user-space
CONFIG_FW_LOADER_USER_HELPER=y
CONFIG_FW_LOADER_USER_HELPER_FALLBACK=y

## Disable Android low memory killer
# CONFIG_HAVE_LOW_MEMORY_KILLER is not set
# CONFIG_ANDROID_LOW_MEMORY_KILLER is not set
# CONFIG_ANDROID_LOW_MEMORY_KILLER_AUTODETECT_OOM_ADJ_VALUES is not set

## sched cgroup_v2
CONFIG_NAMESPACES=y
CONFIG_CGROUPS=y
CONFIG_CGROUP_PIDS=y
CONFIG_CPUSETS=y
CONFIG_CPU_FREQ=y
CONFIG_BPF=y
CONFIG_BPF_SYSCALL=y
CONFIG_CGROUP_BPF=y
CONFIG_FREEZER=y
CONFIG_CGROUP_FREEZER=y
CONFIG_SUSPEND_FREEZER=y
CONFIG_MEMCG=y
CONFIG_SLUB_MEMCG_SYSFS_ON=y
CONFIG_MEMCG_KMEM=y
CONFIG_MEMCG_SWAP=y
CONFIG_MEMCG_SWAP_ENABLED=y
CONFIG_CGROUP_MEM_RES_CTLR=y
CONFIG_CGROUP_MEM_RES_CTLR_SWAP=y
CONFIG_CGROUP_MEM_RES_CTLR_KMEM=y
CONFIG_CGROUP_CPUACCT=y
CONFIG_BLK_CGROUP=y
CONFIG_CGROUP_WRITEBACK=y
CONFIG_CGROUP_DEVICE=y
CONFIG_CGROUP_PERF=y
## governors
CONFIG_CPU_FREQ_DEFAULT_GOV_SCHEDUTIL=y
CONFIG_CPU_FREQ_GOV_SCHEDUTIL=y
## Optimizations
CONFIG_SCHED_MC=y
CONFIG_SCHED_HRTICK=y
## schedulers
CONFIG_CGROUP_SCHED=y
CONFIG_SCHED_WALT=y
CONFIG_SCHED_TUNE=y
CONFIG_SCHED_CORE_CTL=y
CONFIG_SCHED_AUTOGROUP=y
# CONFIG_FAIR_GROUP_SCHED is not set
# CONFIG_RT_GROUP_SCHED is not set
# CONFIG_SCHED_SMT is not set
## io schedulers
CONFIG_IOSCHED_NOOP=y
CONFIG_MQ_IOSCHED_DEADLINE=y
CONFIG_MQ_IOSCHED_KYBER=y
CONFIG_DEFAULT_IOSCHED="deadline"
# CONFIG_IOSCHED_DEADLINE is not set
# CONFIG_IOSCHED_CFQ is not set
# CONFIG_IOSCHED_BFQ is not set
# CONFIG_CFQ_GROUP_IOSCHED is not set
## net cgroup
CONFIG_NETPRIO_CGROUP=y
CONFIG_NET_CLS_CGROUP=y
CONFIG_CGROUP_NET_PRIO=y
CONFIG_CGROUP_NET_CLASSID=y
CONFIG_CGROUP_RDMA=y
CONFIG_SOCK_CGROUP_DATA=y
# CONFIG_NETFILTER_XT_MATCH_CGROUP is not set
## thermal
CONFIG_THERMAL=y
CONFIG_THERMAL_HWMON=y
CONFIG_THERMAL_OF=y
CONFIG_THERMAL_WRITABLE_TRIPS=y
CONFIG_THERMAL_GOV_STEP_WISE=y
CONFIG_THERMAL_DEFAULT_GOV_STEP_WISE=y
CONFIG_THERMAL_EMERGENCY_POWEROFF_DELAY_MS=2000
CONFIG_THERMAL_GOV_USER_SPACE=y
CONFIG_THERMAL_GOV_LOW_LIMITS=y
CONFIG_THERMAL_TSENS=y
# CONFIG_THERMAL_DEFAULT_GOV_FAIR_SHARE is not set
# CONFIG_THERMAL_DEFAULT_GOV_USER_SPACE is not set
# CONFIG_THERMAL_DEFAULT_GOV_POWER_ALLOCATOR is not set
# CONFIG_THERMAL_GOV_FAIR_SHARE is not set
# CONFIG_THERMAL_GOV_BANG_BANG is not set
# CONFIG_THERMAL_GOV_POWER_ALLOCATOR is not set
# CONFIG_THERMAL_EMULATION is not set
## debug
CONFIG_CGROUP_DEBUG=y
CONFIG_SCHED_INFO=y
# CONFIG_DEBUG_BLK_CGROUP is not set
# CONFIG_SCHEDSTATS is not set
# CONFIG_SCHED_DEBUG is not set
